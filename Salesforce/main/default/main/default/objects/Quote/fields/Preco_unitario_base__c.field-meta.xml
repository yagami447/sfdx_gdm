<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Preco_unitario_base__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Quantidade_total_do_produto__c &gt;= Opportunity.Campaign.Kg_para_quarto_preco__c, Opportunity.Campaign.Quarto_Preco_Multiplicacao__c,
IF(Quantidade_total_do_produto__c &gt;= Opportunity.Campaign.Kg_para_terceiro_prec__c, Opportunity.Campaign.Terceiro_Preco_Multiplicacao__c,
IF(Quantidade_total_do_produto__c &gt;= Opportunity.Campaign.Kg_para_segundo_preco__c, Opportunity.Campaign.Segundo_Preco_Multiplicacao__c, Opportunity.Campaign.Primeiro_Preco_Multiplicacao__c)))</formula>
    <label>Preço unitário base</label>
    <precision>18</precision>
    <required>false</required>
