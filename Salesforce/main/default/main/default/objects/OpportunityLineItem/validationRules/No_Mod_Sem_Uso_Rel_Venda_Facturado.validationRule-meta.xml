<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Mod_Sem_Uso_Rel_Venda_Facturado</fullName>
    <active>true</active>
    <errorConditionFormula>AND((PRIORVALUE( Semente_uso_propio__c) &lt;&gt; Semente_uso_propio__c),  ISPICKVAL(Estado_Facturacion__c,&quot;Facturado&quot;))</errorConditionFormula>
    <errorDisplayField>Semente_uso_propio__c</errorDisplayField>
    <errorMessage>No se puede modificar este valor si el relatorio ha sido facturado</errorMessage>
</ValidationRule>
