            <div class="nav navbar-nav navbar-right" role="form">
                <li class="">
                    <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false" data-placement="bottom" title="{!activeAccount.Account__r.Name}">
