                    <ul class="dropdown-menu">
                        <li><a href="javascript:;" onclick="goContract()" style="pointer-events: none;">{!$ObjectType.Contrato__c.LabelPlural}</a></li>
                        <li class="divider"></li>
                        <li><a href="javascript:;" onclick="goApp1()">{!titulo1}</a></li>
