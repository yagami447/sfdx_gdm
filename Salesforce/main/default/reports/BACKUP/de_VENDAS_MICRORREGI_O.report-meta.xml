    </aggregates>
    <aggregates>
        <calculatedFormula>( OpportunityLineItem.Semente_comercializada__c:SUM + OpportunityLineItem.Semente_uso_propio__c:SUM )  /  ( PARENTGROUPVAL(OpportunityLineItem.Semente_comercializada__c:SUM, Account.Region__c) + PARENTGROUPVAL(OpportunityLineItem.Semente_uso_propio__c:SUM, Account.Region__c) )</calculatedFormula>
