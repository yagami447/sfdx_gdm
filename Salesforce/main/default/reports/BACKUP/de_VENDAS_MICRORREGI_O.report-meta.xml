    </aggregates>
    <aggregates>
        <calculatedFormula>( OpportunityLineItem.Semente_comercializada__c:SUM + OpportunityLineItem.Semente_uso_propio__c:SUM )  /  ( PARENTGROUPVAL(OpportunityLineItem.Semente_comercializada__c:SUM, GRAND_SUMMARY) + PARENTGROUPVAL(OpportunityLineItem.Semente_uso_propio__c:SUM, GRAND_SUMMARY) )</calculatedFormula>
