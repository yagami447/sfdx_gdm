        <isCrossBlock>false</isCrossBlock>
        <masterLabel>EFICIÊNCIA</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>( OpportunityLineItem.Semente_aprovada__c:SUM -  ( OpportunityLineItem.Semente_comercializada__c:SUM + OpportunityLineItem.Semente_uso_propio__c:SUM )  )  / OpportunityLineItem.Semente_aprovada__c:SUM</calculatedFormula>
