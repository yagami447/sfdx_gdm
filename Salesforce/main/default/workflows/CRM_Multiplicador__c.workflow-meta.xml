    <fieldUpdates>
        <fullName>Actualizar_Pontos_Obj_Gen_Dem_NEOG</fullName>
        <field>NEOG_Pontos_Obj_Gen_Dem__c</field>
        <formula>((NG_Palestras_Realizado__c + NG_Treinamentos_Realizado__c + NG_Participacao_dia_campo_realizado__c) * IF(NG_Categoria_resultante__c = &quot;STANDARD&quot;, 100, IF(NG_Categoria_resultante__c = &quot;PLUS&quot;, 500, 1000))) + IF(CONTAINS(Region_Comercial_Copia__c,&quot;REGION&quot;),
(NG_LXL_Realizado_Plantado__c * IF(NG_Categoria_resultante__c = &quot;STANDARD&quot;, 0, IF(NG_Categoria_resultante__c = &quot;PLUS&quot;, 1500, 3000))),0) + (NG_LXL_Realizado_Colhido__c * IF(NG_Categoria_resultante__c = &quot;STANDARD&quot;, 300, IF(NG_Categoria_resultante__c = &quot;PLUS&quot;, 1500, 3000))) + (NG_Identifica_o_de_beiras_de_rodovia__c * IF(NG_Categoria_resultante__c = &quot;STANDARD&quot;, 200, IF(NG_Categoria_resultante__c = &quot;PLUS&quot;, 1000, 2000))) + (NG_Outdoor_Realizado__c * IF(NG_Categoria_resultante__c = &quot;STANDARD&quot;, 400, IF(NG_Categoria_resultante__c = &quot;PLUS&quot;, 2000, 4000)))</formula>
        <name>Actualizar Pontos Obj Gen Dem NEOG</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Actualizar_Pontos_Vendas_BRMX</fullName>
        <field>BR_Pontos_volume_2__c</field>
        <formula>IF(OR(Region_Comercial__c=&quot;REGION 1&quot;, Region_Comercial__c=&quot;REGION 2&quot;),

IF(BR_SemComer_Safra_actual__c&gt;220000,16,IF(BR_SemComer_Safra_actual__c&gt;120000, 12, IF(BR_SemComer_Safra_actual__c &gt;70000, 6, IF(BR_SemComer_Safra_actual__c &gt;40000, 3, 0)))),

IF(BR_SemComer_Safra_actual__c&gt;550000,18,IF(BR_SemComer_Safra_actual__c&gt;350000,16,IF(BR_SemComer_Safra_actual__c&gt;270000, 12, IF( BR_SemComer_Safra_actual__c &gt;200000, 6, IF( BR_SemComer_Safra_actual__c &gt;130000, 3, 0)))))

)</formula>
        <name>Actualizar Pontos Vendas BRMX</name>
