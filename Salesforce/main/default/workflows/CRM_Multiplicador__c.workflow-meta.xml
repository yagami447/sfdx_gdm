    <fieldUpdates>
        <fullName>Actualizar_Pontos_Vendas_NEOG</fullName>
        <field>NG_Pontos_volume__c</field>
        <formula>IF(OR(Region_Comercial__c=&quot;REGION 1&quot;, Region_Comercial__c=&quot;REGION 2&quot;),

IF(NG_Soma_de_Sem_Comercializada__c&gt;30000,IF(NG_Categoria_resultante__c=&quot;PREMIUM&quot;, 1, IF(NG_Categoria_resultante__c=&quot;PLUS&quot;, 0.5, 0.25)),
IF(NG_Soma_de_Sem_Comercializada__c&gt;10000,IF(NG_Categoria_resultante__c=&quot;PREMIUM&quot;, 0.5, IF(NG_Categoria_resultante__c=&quot;PLUS&quot;, 0.25,0)),0)),

 
IF(NG_Soma_de_Sem_Comercializada__c&gt;60000,IF(NG_Categoria_resultante__c=&quot;PREMIUM&quot;, 1, IF(NG_Categoria_resultante__c=&quot;PLUS&quot;, 0.5, 0.25)),
IF(NG_Soma_de_Sem_Comercializada__c&gt;30000,IF(NG_Categoria_resultante__c=&quot;PREMIUM&quot;, 0.5, IF(NG_Categoria_resultante__c=&quot;PLUS&quot;, 0.25,0)),0))

)</formula>
        <name>Actualizar Pontos Vendas NEOG</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
